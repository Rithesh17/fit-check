  "dependencies": {
    "statue-ssg": "file:../statue/statue-ssg-0.2.15.tgz",
    "@supabase/supabase-js": "^2.39.0",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "lucide-svelte": "^0.561.0",
