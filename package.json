{
  "name": "fit-check",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npx -y serve build",
    "prepare": "svelte-kit sync || echo ''",
