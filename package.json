    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^4.2.3",
    "@types/node": "^22.13.13",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.20.2",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
